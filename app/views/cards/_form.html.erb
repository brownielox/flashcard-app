<h2>Make a new card</h2>
<%= form_for @card do |f| %>
  <%= f.label :subject_name %>
  <br>
  <%= f.text_field :subject_name %>
  <br>
  <%= f.label :front %>
  <br>
  <%= f.text_field :front %>
  <br>
  <%= f.label :back %>
  <br>
  <%= f.text_field :back %>
  <br>
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>


<%= submit_tag "Submit Card" %>
<% end %>
